<?xml version="1.0"?>
<configuration>
    
    <startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8"/></startup>

    <appSettings>

        <add key="inputFolder" value="C:\Temp\ZipFiles"/>
        
        <add key="zipFileRegexPattern" value=".*"/>
        
        <add key="password" value=""/>

        <add key="zipEntryRegexPattern" value=".*"/>

        <add key="outputFolder" value="C:\Temp\Output"/>

        <add key="createSubFolders" value="True"/>

        <!-- set to: "overwrite", "skip", or "error" -->
        <add key="existingFileAction" value="skip"/>

    </appSettings>


</configuration>
